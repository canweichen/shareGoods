/* pages/tool/tool.wxss */
page {
  background-color: #eee;
}
.selTool{
  margin: 10px 2% 0 2%;
  width: 96%;
  background: white;
  border-radius: 10px;
}
.contain{
  height:35px;
  line-height:35px;
  font-size:16px;
  padding-left:15px;
  border-bottom:1px solid #eee;
}
.selTool navigator{
  border: none;
}
.weui-grids{
  border:none;
}
.weui-grid{
  width:25%;
}
.weui-grid__icon{
  width:50px;
  height: 50px;
}
